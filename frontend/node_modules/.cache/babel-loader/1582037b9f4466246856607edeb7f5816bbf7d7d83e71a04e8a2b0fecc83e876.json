le)\r\n            showLogin.value = false\r\n            showSignUp.value = false\r\n            location.reload()\r\n          } else {\r\n            alert('Â∏≥ËôüÊàñÂØÜÁ¢ºÈåØË™§')\r\n          }\r\n        })\r\n    }\r\n    \r\n    const handleSignUp = ({ username, password }) => {\r\n      axios.post('http://127.0.0.1:5000/signup', { username, password }, { withCredentials: true })\r\n        .then(res => {\r\n          if (res.data.success) {\r\n            alert('Ë®ªÂÜäÊàêÂäü')\r\n            location.reload()\r\n          } else {\r\n            alert('Â∏≥ËôüÂ∑≤Â≠òÂú®')\r\n          }\r\n        })\r\n    }\r\n\r\n    const logout = () => {\r\n      axios.post('http://localhost:5000/logout', {}, { withCredentials: true })\r\n        .then(() => {\r\n          userStore.logout()\r\n          if(route.path.startsWith('/admin')){\r\n            router.push(`/`)\r\n          }\r\n          else{\r\n            location.reload()\r\n          }\r\n        })\r\n    }\r\n\r\n    return {\r\n      showLogin,\r\n      showSignUp,\r\n      userStore,\r\n      handleSignUp,\r\n      handleLogin,\r\n      logout\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n  \r\n\r\n<style scoped>\r\n\r\n.nav-link {\r\n  font-size: 20px;\r\n  text-decoration: none;\r\n  color: #8b0aed;\r\n  font-weight: bold;\r\n  margin-left: 12px;\r\n}\r\n\r\n\r\n.nav-link:first-child {\r\n  margin-left: 0;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC,0CAA0C;EAAC,eAAa,EAAC;;;EAC7DA,KAAK,EAAC;AAAiB;;EACpBA,KAAK,EAAC,mDAAmD;EAACC,IAAI,EAAC;;;EAOhED,KAAK,EAAC;AAA2B;;EAE5BA,KAAK,EAAC;AAAgB;;;;6DAXpCE,mBAAA,CAoBM,OApBNC,UAoBM,GAnBJD,mBAAA,CAkBM,OAlBNE,UAkBM,GAjBJF,mBAAA,CAA2F,QAA3FG,UAA2F,EAAAC,gBAAA,CAAfC,MAAA,CAAAC,KAAK,O,0BACjFN,mBAAA,CAIK;IAJDF,KAAK,EAAC;EAAiC,IACzCE,mBAAA,CAEK;IAFDF,KAAK,EAAC;EAAU,IAClBE,mBAAA,CAA+D;IAA5DF,KAAK,EAAC,iBAAiB;IAAC,cAAY,EAAC,MAAM;IAACC,IAAI,EAAC;KAAI,KAAG,E,UAI/DC,mBAAA,CASM,OATNO,UASM,GARYC,MAAA,CAAAC,SAAS,CAACC,UAAU,I,cAApCC,mBAAA,CAGWC,SAAA;IAAAC,GAAA;EAAA,IAFTb,m